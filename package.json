{
  "name": "gcp-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "build": "npm run build --workspace=backend && npm run build --workspace=frontend",
    "redev": "npm run build && npm run dev",
    "deploy": "bash ./deploy.sh"
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}